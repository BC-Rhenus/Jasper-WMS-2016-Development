	<parameter name="pallet_id" class="java.lang.String"/>
	<parameter name="container_id" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select distinct 1 seq,odh.client_id,odh.order_id,spi.line_id,spi.sequence,spi.code,spi.text
from dcsdba.order_header odh
left join dcsdba.special_ins spi on odh.order_id=spi.reference_id and odh.client_id=spi.client_id
left join dcsdba.special_ins_code ins on spi.code=ins.code and spi.client_id=ins.client_id
left join dcsdba.order_container ocr on odh.order_id=ocr.order_id and odh.client_id=ocr.client_id
where odh.client_id=$P{client_id}
and (odh.order_id=$P{order_id} or $P{order_id} is null)
and (ocr.pallet_id=$P{pallet_id} or $P{pallet_id} is null)
and (ocr.container_id=$P{container_id} or $P{container_id} is null)
union all
select distinct 2 seq,'---' client_id,'---' order_id,null line_id,null sequence,'---' code,'---' text
from dual
union all
select distinct 3 seq,odh.client_id,odh.order_id,null line_id,null sequence,'container' code,ocr2.container_id text
from dcsdba.order_header odh
left join dcsdba.order_container ocr on odh.order_id=ocr.order_id and odh.client_id=ocr.client_id
left join dcsdba.order_container ocr2 on odh.order_id=ocr2.order_id and odh.client_id=ocr2.client_id
where odh.client_id=$P{client_id}
and (odh.order_id=$P{order_id} or $P{order_id} is null)
and (ocr.pallet_id=$P{pallet_id} or $P{pallet_id} is null)
and (ocr.container_id=$P{container_id} or $P{container_id} is null)
order by 1,2,3,4,5]]>
	</queryString>
